{
    "env": {
      "development": {
        "presets": [
          "next/babel",
          "@zeit/next-typescript/babel"
        ],
        "plugins": [
          "transform-class-properties",
          ["@babel/plugin-proposal-decorators", { "legacy": true }]
        ]
      },
      "production": {
        "presets": [
          "next/babel",
          "@zeit/next-typescript/babel"
        ],
        "plugins": [
          "transform-class-properties",
          ["@babel/plugin-proposal-decorators", { "legacy": true }]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ],
          "@zeit/next-typescript/babel"
        ],
        "plugins": [
          "transform-class-properties",
          ["@babel/plugin-proposal-decorators", { "legacy": true }]
        ]
      }
    }
  }